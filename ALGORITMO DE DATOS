#include<iostream>
#include<vector>
#include<algorithm>
#include<stdlib.h>
#include<time.h>
#include<conio.h>
#include<string>

using namespace std;
using namespace System;
template<typename T>
T sumar(T a, T b)
{
	return a + b;
}
int main()
{
	cout << sumar(5, 6) << endl;
	cout << sumar(5.7, 6.1) << endl;
	cout << sumar(5.7f, 6.1f) << endl;
	cout << sumar(' ', ' ') << endl;
	cout << sumar<string>("hola", "Mundo!") << endl;
	getch();
}
